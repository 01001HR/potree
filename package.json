{
  "name": "potree",
  "private": true,
  "version": "0.0.1",
  "description": "WebGL point cloud viewer",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src",
    "test": "npm run lint",
    "p": "./node_modules/.bin/run-p --aggregate-output",
    "b": "./node_modules/.bin/browserify",
    "cpx": "./node_modules/.bin/cpx",
    "copy": "npm run p -- c-resources c-license c-extra",
    "c-resources": "npm run cpx -- \"resources/**/*\" build/potree/resources",
    "c-license": "npm run cpx -- LICENSE build/potree",
    "c-extra": "npm run cpx -- \"src/viewer/{potree.css,profile.html,sidebar.html}\" build/potree",
    "b-worker": "    npm run p -- b-worker-bin b-worker-dem b-worker-las b-worker-laz b-worker-gre",
    "b-main": "      npm run b -- -o build/potree/potree.js                               -e ./src/index.js                                -s Potree",
    "b-worker-bin": "npm run b -- -o build/potree/workers/BinaryDecoderWorker.js          -e ./src/workers/BinaryDecoderWorker.js",
    "b-worker-dem": "npm run b -- -o build/potree/workers/DEMWorker.js                    -e ./src/workers/DEMWorker.js",
    "b-worker-gre": "npm run b -- -o build/potree/workers/GreyhoundBinaryDecoderWorker.js -e ./src/workers/GreyhoundBinaryDecoderWorker.js --external ws",
    "b-worker-las": "npm run b -- -o build/potree/workers/LASDecoderWorker.js             -e ./src/workers/LASDecoderWorker.js             --external ws",
    "b-worker-laz": "npm run b -- -o build/potree/workers/LASLAZWorker.js                 -e ./libs/plasio/workers/laz-loader-worker.js    --external ws",
    "b-all": "npm run p -- b-main b-worker"
  },
  "main": "./src/index.js",
  "browser": "./src/index.js",
  "browserify": {
    "transform": [
      "browserify-shader"
    ]
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.1.0",
    "d3": "^4.10.2",
    "i18next-client": "^1.11.4",
    "jquery": "^3.2.1",
    "openlayers": "^3.15.1",
    "process-nextick-args": "^1.0.7",
    "proj4": "^2.3.12",
    "safe-buffer": "^5.1.1",
    "shapefile": "^0.6.2",
    "stats.js": "^0.17.0",
    "three": "^0.87.1",
    "three-ply-loader": "^1.0.0",
    "util-deprecate": "^1.0.2"
  },
  "author": "Markus Sch√ºtz",
  "license": "BSD-2-CLAUSE",
  "devDependencies": {
    "browserify": "^14.4.0",
    "browserify-shader": "^0.1.2",
    "cpx": "^1.5.0",
    "eslint": "^4.4.1",
    "eslint-config-happiness": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "~3.5.2",
    "gulp-concat": "~2.1.7",
    "gulp-connect": "^2.3.1",
    "gulp-jshint": "^2.0.4",
    "gulp-rename": "~1.0.0",
    "gulp-size": "^2.1.0",
    "gulp-uglify": "~0.2.1",
    "gulp-util": "~2.2.20",
    "jshint": "^2.9.5",
    "npm-run-all": "^4.1.1",
    "through": "~2.3.4",
    "workerify": "^1.0.0"
  }
}
